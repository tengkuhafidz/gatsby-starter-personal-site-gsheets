{
  "name": "gatsby-starter-personal-site-gdocs",
  "private": true,
  "description": "This is a Gatsby starter for personal site that is uses Google Sheets and Google Docs as CMS",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "gatsby": "^2.21.0",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "lint-staged": "^10.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.8",
    "eslint": "^5.14",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "tailwindcss": "^1.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix"
    ]
  }
}
